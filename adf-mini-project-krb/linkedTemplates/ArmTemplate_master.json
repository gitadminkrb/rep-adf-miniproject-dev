{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "adf-mini-project-krb"
		},
		"ls_AzDatabricks_cognitivesearch_accessToken": {
			"type": "secureString",
			"metadata": "Secure string for 'accessToken' of 'ls_AzDatabricks_cognitivesearch'"
		},
		"ls_adlsgen2_blob_accountKey": {
			"type": "secureString",
			"metadata": "Secure string for 'accountKey' of 'ls_adlsgen2_blob'"
		},
		"ls_adlsgen2_blob_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://adlsgen2miniprojectkrb.dfs.core.windows.net"
		},
		"ls_googleapi_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://newsapi.org/v2/"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "adf-mini-project-krb_ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2018-02-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"ls_AzDatabricks_cognitivesearch_accessToken": {
						"value": "[parameters('ls_AzDatabricks_cognitivesearch_accessToken')]"
					},
					"ls_adlsgen2_blob_accountKey": {
						"value": "[parameters('ls_adlsgen2_blob_accountKey')]"
					},
					"ls_adlsgen2_blob_properties_typeProperties_url": {
						"value": "[parameters('ls_adlsgen2_blob_properties_typeProperties_url')]"
					},
					"ls_googleapi_properties_typeProperties_url": {
						"value": "[parameters('ls_googleapi_properties_typeProperties_url')]"
					}
				}
			},
			"dependsOn": []
		}
	]
}